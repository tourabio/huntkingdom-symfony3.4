<script type="text/javascript">
    $("#formCompetition").submit(function(e){

        /**** NOM *****/
        if ( $("#nom").val()==""){
            $("#errorNom").show();
            e.preventDefault();

        }
        else
        {
            $("#errorNom").hide();
        }


        /**** categorie *****/

        if ( $("#categorie").val()==""){
            $("#errorCategorie").show();
            e.preventDefault();

        }
        else
        {
            $("#errorCategorie").hide();
        }


        /**** Lieu *****/

        if ( $("#lieu").val()==""){
            $("#errorlieu").show();
            e.preventDefault();
        }
        else
        {
            $("#errorlieu").hide();
        }
        /**** Lieu *****/

        if ( $("#titre").val()==""){
            $("#errortitre").show();
            e.preventDefault();
        }
        else
        {
            $("#errortitre").hide();
        }



        /**** nbParticipants *****/
        if ( $("#nbParticipants").val()==""){
            $("#errornbParticipants").show();
            $("#errornbParticipants2").hide();
            e.preventDefault();

        }
        else if ( $("#nbParticipants").val()<=2){
            $("#errornbParticipants2").show();
            $("#errornbParticipants").hide();
            e.preventDefault();
        }
        else
        {
            $("#errornbParticipants").hide();
